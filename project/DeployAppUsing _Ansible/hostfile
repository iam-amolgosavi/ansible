# Ex 2: A collection of hosts belonging to the 'webservers' group:
#vim sudo /etc/ansible/hosts
[servers]
host1 ansible_host=3.137.218.69
host2 ansible_host=3.21.98.120

[all:vars]
ansible_python_interpreter=/usr/bin/python3
ansible_user=ubuntu
ansible_ssh_private_key_file=/home/ubuntu/keys/ansi-key.pem
